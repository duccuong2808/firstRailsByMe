		<!-- MAIN -->
		<div id="main">
			<div class="wrapper cf">
			
				
				<!-- posts list -->
	        	<div id="posts-list" class="cf">        	
					
					<!-- Image -->
				<% @blogcrs.each do |blogcr| %>
					<% if blogcr.attach_content_type == "image/jpeg" || blogcr.attach_content_type == "image/png" || blogcr.attach_content_type == "image/gif" %>
					<!-- begin image -->
					<article class="format-image">
						<div class="feature-image">
							<a href="<%= blogcr.attach.url %>" data-rel="prettyPhoto">
								<img src="<%= blogcr.attach.url %>" alt="Alt text" />
							</a>
						</div>
						<div class="box cf">
							<div class="entry-date"><div class="number"><%= blogcr.created_at.strftime("%d")%></div><div class="month"><%= blogcr.created_at.strftime("%b")%></div></div>
							
							<div class="excerpt">
								<a href="#" class="post-heading" ><%= blogcr.title %></a>
								<p><%= blogcr.content%></p>
								
								
							</div>
							
							<div class="meta">
								<span class="format">
									<% @sortbgs.each do |sortbg|%>
										<% if blogcr.sortbgs_id.to_i == sortbg.id %>
										<%= sortbg.namesortbg %>
										<% end %>
									<% end %>
								</span>
								<span class="user"><a href="#">By <%= blogcr.creater%> </a></span>
							<!-- 	<span class="comments">16 comments</span> -->
								<span class="tags"><%= raw blogcr.tag_list.map { |t| link_to t, tager_path(t) }.join(', ') %></span>
							</div>
								
						</div>
					</article>
					<% end %>
					<!-- end image -->
					<!-- begin audio -->
					<% if blogcr.attach_content_type == "audio/mp3" %>
					<article class="format-audio">
						<div class="box cf">
							<div class="entry-date"><div class="number"><%= blogcr.created_at.strftime("%d")%></div><div class="month"><%= blogcr.created_at.strftime("%b")%></div></div>
							
							<div class="excerpt">
								<a href="#" class="post-heading" ><%= blogcr.title %></a>

									<audio controls="controls">  
										<source src="<%= blogcr.attach.url %>" /> 
									</audio> 
							<div><br><%= blogcr.content %></div>
							</div>
							
							<div class="meta">
								<span class="format">
									<% @sortbgs.each do |sortbg|%>
										<% if blogcr.sortbgs_id.to_i == sortbg.id %>
										<%= sortbg.namesortbg %>
										<% end %>
									<% end %>
								</span>
								<span class="user"><a href="#">By <%= blogcr.creater%> </a></span>
								<!-- <span class="comments">16 comments</span> -->
								<span class="tags"><%= raw blogcr.tag_list.map { |t| link_to t, tager_path(t) }.join(', ') %></span>
							</div>
								
						</div>
					</article>
					<!-- ENDS Audio -->
					<% end %>
					<!-- begin video -->
					<% if blogcr.attach_content_type == "video/mp4" %>
					<article class="format-video">
						<div class="box cf">
							<div class="entry-date"><div class="number"><%= blogcr.created_at.strftime("%d")%></div><div class="month"><%= blogcr.created_at.strftime("%b")%></div></div>
							
							<div class="excerpt">
								
									<video width="320" height="240" controls>
										<source src="<%= blogcr.attach.url %>" type="video/mp4">
  										Your browser does not support the video tag.
									</video>
									<a href="#" class="post-heading" ><%= blogcr.title %></a>
									<div><br><%= blogcr.content %></div>
							</div>
							
							<div class="meta">
								<span class="format">
									<% @sortbgs.each do |sortbg |%>
										<% if sortbg.id == blogcr.sortbgs_id.to_i %>
											<%= sortbg.namesortbg %>
										<% end %>
									<% end %>
								</span>
								<span class="user"><a href="#">By <%= blogcr.creater%> </a></span>
								<!-- <span class="comments">16 comments</span> -->
								<span class="tags"><%= raw blogcr.tag_list.map { |t| link_to t, tager_path(t) }.join(', ') %></span>
							</div>
								
						</div>
					</article>
					<% end %>
					<!-- end video -->
					<% if blogcr.attach_content_type != "video/mp4" && blogcr.attach_content_type != "audio/mp3" && blogcr.attach_content_type != "video/mp4" && blogcr.attach_content_type != "image/jpeg" && blogcr.attach_content_type != "image/png" && blogcr.attach_content_type != "image/gif"%>
					<article class="format-standard">
<!-- 						<div class="feature-image">
							<a href="single.html">
								<img src="img/dummies/slides/01.jpg" alt="Alt text" />
							</a>
						</div> -->
						<div class="box cf">
							<div class="entry-date"><div class="number"><%= blogcr.created_at.strftime("%d")%></div><div class="month"><%= blogcr.created_at.strftime("%b")%></div></div>
							
							<div class="excerpt">
								<a href="/blog/<%= blogcr.id %>" class="post-heading" ><%= blogcr.title %></a>
								<p><%= blogcr.content %></p>
								
								<p><a href="/blog/<%= blogcr.id %>" class="learnmore">Learn More</a></p>
							</div>
							
							<div class="meta">
								<span class="format">
									<% @sortbgs.each do |sortbg |%>
										<% if sortbg.id == blogcr.sortbgs_id.to_i %>
											<%= sortbg.namesortbg %>
										<% end %>
									<% end %>
								</span>
								<span class="user"><a href="#">By <%= blogcr.creater%> </a></span>
								<!-- <span class="comments">16 comments</span> -->
								<span class="tags"><%= raw blogcr.tag_list.map { |t| link_to t, tager_path(t) }.join(', ') %></span>
							</div>
								
						</div>
					</article>
					<% end %>
				<% end %>
					
					
					
					<!-- page-navigation -->
<!-- 					<div class="page-navigation cf">
